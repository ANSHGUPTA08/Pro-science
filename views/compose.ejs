<%- include('partials/header'); -%>

<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="postTittle">Tittle</label>
    <input class="form-control input-sm" type="text" name="postTittle">
  </div>
  <div class="form-group">
    <label for="postBody">Post</label>
    <textarea class="form-control input-lg inputbig" type="text" name="postBody" rows="8" cols="80"></textarea>
  </div>
  <button class="btn btn-md btn-primary" type="submit" name="button">Publish</button>
</form>

<%- include('partials/footer'); -%>
